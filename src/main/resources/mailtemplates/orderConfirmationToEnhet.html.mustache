{{<index.html.mustache}}
  {{$content}}

    {{<primitives/h1}}
      {{$content}}
        {{labels.orderConfirmationToEnhetTitle}}
      {{/content}}
    {{/primitives/h1}}

    {{<primitives/container}}
      {{$content}}
        {{<primitives/paragraph}}
          {{$content}}

          {{ labels.orderConfirmationToEnhetIntro }}

          {{labels.orderConfirmationToEnhetLabel}}: {{enhet.navn}}<br>
          {{labels.orderConfirmationToEnhetOrderDateLabel}}: {{innsynskrav.opprettetDato}}<br>

          Svar på dette innsynskravet ønskes sendt med epost til: <strong>{{innsynskrav.epost}}</strong><br>

          {{/content}}
        {{/primitives/paragraph}}
      {{/content}}
    {{/primitives/container}}

    {{<primitives/container}}
      {{$content}}
        {{<primitives/h2}}
          {{$content}}{{labels.orderConfirmationToEnhetDocuments}}{{/content}}
        {{/primitives/h2}}

        {{#innsynskravDelList}}
          {{<primitives/container}}
            {{$content}}
              {{<primitives/paragraph}}
                {{$content}}
                  Saksnr: {{journalpost.saksmappe.saksaar}}/{{journalpost.saksmappe.sakssekvensnummer}} | Dok nr. : {{journalpost.journalpostnummer}} | Sekvensnr.: {{journalpost.journalsekvensnummer}}<br>
                  Sak: {{journalpost.saksmappe.offentligTittelSensitiv}}<br>
                  Dokument: {{journalpost.offentligTittelSensitiv}}<br>
                  Dok.dato: {{journalpost.dokumentdato}}<br>
                  Journaldato: {{journalpost.journaldato}}<br>
                  Saksbehandler: {{journalpost.saksbehandler}}<br>
                  Enhet: {{journalpost.administrativEnhet}}<br>
                {{/content}}
              {{/primitives/paragraph}}
            {{/content}}
          {{/primitives/container}}
        {{/innsynskravDelList}}

      {{/content}}
    {{/primitives/container}}

    {{<primitives/container}}
      {{$content}}
        {{<primitives/paragraph}}
          {{$content}}{{ labels.orderConfirmationToEnhetOutro }}{{/content}}
        {{/primitives/paragraph}}
      {{/content}}
    {{/primitives/container}}

  {{/content}}
{{/index.html.mustache}}
