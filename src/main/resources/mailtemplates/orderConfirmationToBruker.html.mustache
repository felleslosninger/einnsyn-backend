{{<index.html.mustache}}
  {{$content}}

    {{<primitives/h1}}
      {{$content}}
        {{labels.orderConfirmationToBrukerTitle}}
      {{/content}}
    {{/primitives/h1}}

    {{<primitives/container}}
      {{$content}}
        {{<primitives/paragraph}}
          {{$content}}
            {{^innsynskravList.isEmpty}}
              {{ labels.orderConfirmationToBrukerIntro }}
            {{/innsynskravList.isEmpty}}
            {{^innsynskravList}}
              {{labels.orderConfirmationToBrukerEmpty}}
            {{/innsynskravList}}
          {{/content}}
        {{/primitives/paragraph}}
      {{/content}}
    {{/primitives/container}}

    {{^innsynskravList.isEmpty}}
      {{<primitives/container}}
        {{$content}}
          {{<primitives/h2}}
            {{$content}}{{labels.orderConfirmationToBrukerDocuments}}{{/content}}
          {{/primitives/h2}}

          {{#innsynskravList}}
            {{<primitives/container}}
              {{$content}}
                {{<primitives/paragraph}}
                  {{$content}}
                    Saksnr: {{journalpost.saksmappe.saksaar}}/{{journalpost.saksmappe.sakssekvensnummer}} | Dok nr. : {{journalpost.journalpostnummer}} | Sekvensnr.: {{journalpost.journalsekvensnummer}}<br>
                    Dokument: {{journalpost.offentligTittelSensitiv}}<br>
                    Dok.dato: {{journalpost.dokumentetsDato}}<br>
                  {{/content}}
                {{/primitives/paragraph}}
              {{/content}}
            {{/primitives/container}}
          {{/innsynskravList}}

        {{/content}}
      {{/primitives/container}}
    {{/innsynskravList.isEmpty}}

    {{<primitives/container}}
      {{$content}}
        {{<primitives/paragraph}}
          {{$content}}{{ labels.orderConfirmationToBrukerOutro }}{{/content}}
        {{/primitives/paragraph}}
      {{/content}}
    {{/primitives/container}}

  {{/content}}
{{/index.html.mustache}}
